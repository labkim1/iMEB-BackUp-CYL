<Controls:MetroWindow
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ni="http://schemas.ni.com/controls/2009/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        xmlns:TLog="clr-namespace:TLog"
        xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"   
        xmlns:wpg="clr-namespace:System.Windows.Controls"
        mc:Ignorable="d" x:Class="BCPT.MainWindow"
        ShowIconOnTitleBar="True" ShowTitleBar="True" ShowDialogsOverTitleBar="True"  
        Title="iMEB Backup Cylinder Performance Test" WindowStartupLocation="CenterScreen"  Height="1000" Width="1280"   Icon="iMEB.ico" Closing="Window_Closing" Loaded="MetroWindow_Loaded"  >
    <Controls:MetroWindow.Resources>
        <ResourceDictionary>
            <TLog:NullVisibilityConverter x:Key="NullVisibilityConverter" />
            <TLog:VisibilityBoolConverter x:Key="VisibilityBoolConverter" />
            <TLog:FreezeTextConverter     x:Key="FreezeTextConverter" />
            <TLog:NullBooleanConverter    x:Key="NullBooleanConverter" />
        </ResourceDictionary>
    </Controls:MetroWindow.Resources>
    <Controls:MetroWindow.RightWindowCommands>
        <Controls:WindowCommands>
            <Button x:Name="SystemConfig" Content="관리자" Click="SystemConfig_Click"  />
        </Controls:WindowCommands>
    </Controls:MetroWindow.RightWindowCommands>
    <Grid
        Margin="3"
        x:Name="MainGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="220" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="130"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="AUTO"/>
            <RowDefinition Height="32"/>
        </Grid.RowDefinitions>
        <Popup x:Name="NGPopup" Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="1" Grid.RowSpan="3" 
                Placement="Center" AllowsTransparency="True" StaysOpen="False" PlacementTarget="{Binding RelativeSource={RelativeSource AncestorType=Window}}" >
            <Grid>
                <Border Width="450" Height="300" BorderBrush="Blue" BorderThickness="1" Background="#FFB0031A" >
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="60" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="60"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="30"/>
                        </Grid.RowDefinitions>
                        <Image Grid.Column="0" Grid.Row="0" Source="pack://siteoforigin:,,,/Resources/Warning.png"  />
                        <Label Grid.Column="1" Grid.Row="0" FontSize="24" Margin="20,0,0,0" VerticalAlignment="Center" FontWeight="Bold" FontStyle="Italic">NG 및 알람 정보 표시</Label>
                        <ScrollViewer Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="1" Margin="2,2,2,2" >
                            <StackPanel Orientation="Vertical" Margin="3,3,3,3" >                                                      
                                <TextBlock  x:Name="NGMessage1" FontSize="16" > </TextBlock>                            
                            </StackPanel>
                        </ScrollViewer>
                        <Label Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="2"> 본 화면은 자동으로 사라집니다. 자동생산, 수동변경시 ...</Label>
                    </Grid>
                </Border>
            </Grid>
        </Popup>
        <Popup x:Name="OKPopup" Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="1" Grid.RowSpan="3" Placement="Center" AllowsTransparency="True">
            <Grid>
                <Border Width="600" Height="300" BorderBrush="Blue" BorderThickness="1" Background="Honeydew" >
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="60" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="60"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="30"/>
                        </Grid.RowDefinitions>
                        <Image Grid.Column="0" Grid.Row="0"  />
                        <Label Grid.Row="0" Foreground="Blue" FontSize="40" Margin="4,0,76,0" VerticalAlignment="Center" FontWeight="Bold" FontStyle="Italic" Grid.ColumnSpan="2">제품 OK</Label>
                        <ScrollViewer Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="1" Margin="2,2,2,2" >
                            <StackPanel Orientation="Vertical" >
                                <TextBlock Margin="10,20,10,10" x:Name="OKMessage1" FontSize="25" ></TextBlock>
                            </StackPanel>
                        </ScrollViewer>

                    </Grid>
                </Border>
                <Button x:Name="okpopup_close" Content="X" VerticalAlignment="Top" Margin="572,4,5,0" Click="okpopup_close_Click"/>
            </Grid>
        </Popup>
        <Grid Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="0" x:Name="HeaderGrid" Background="White">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="220" />
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="360" />
                <ColumnDefinition Width="360" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="40"/>
                <RowDefinition Height="30"/>
            </Grid.RowDefinitions>

            <!-- 왼쪽 상단 로그 및 이미지-->
            <Grid Grid.Column="0" Grid.Row="0" Grid.RowSpan="3"  >
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="40"/>
                    <RowDefinition Height="30"/>
                </Grid.RowDefinitions>
                <Border Grid.Row="0" Grid.RowSpan="2"  BorderThickness="1" Margin="2,2,2,2">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>
                    <Image x:Name="Title_Image1" Source="pack://siteoforigin:,,,/Resources/BackupCylinder.png"  />
                </Border>
                <Border Grid.Row="3"  BorderThickness="1" Margin="1,1,1,1">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>
                    <Image x:Name="Title_Image2"  Source="pack://siteoforigin:,,,/Resources/colorTone_img1.jpg" />
                </Border>
            </Grid>

            <Grid Grid.Column="1" Grid.ColumnSpan="3" Grid.Row="0" Grid.RowSpan="1" x:Name="TitleGrid" Background="#FF004787">
                <Border BorderThickness="1" Background="#FF004787" >
                    <Label x:Name="L_Title" Content="iMEB BACKUP CYLINDER PERFORMANCE TEST Ver 1.602" FontSize="20" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Background="{x:Null}" Foreground="White"/>
                </Border>
            </Grid>
            <Grid Grid.Column="1" Grid.ColumnSpan="3" Grid.Row="1" x:Name="TitileSubGrid" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.6*" />
                    <ColumnDefinition Width="1*" />
                    <ColumnDefinition Width="0.7*" />
                    <ColumnDefinition Width="2*" />
                </Grid.ColumnDefinitions>
                <Border Grid.Column="0" BorderThickness="1">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <iconPacks:PackIconMaterial Margin="5,0,0,0" Kind="EarthBox" Foreground="Black" VerticalAlignment="Center" HorizontalAlignment="Left" />
                        <Label Content="PRODUCT" VerticalContentAlignment="Center"/>
                        <Label x:Name="D_ProductCode" Content="H1" Width="35" VerticalContentAlignment="Center" HorizontalContentAlignment="Right" VerticalAlignment="Stretch" Foreground="Blue" FontSize="14" FontWeight="Bold"/>
                    </StackPanel>
                </Border>
                <Border Grid.Column="1" BorderThickness="1" Margin="1,0,1,0">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <iconPacks:PackIconMaterial Margin="5,0,0,0"  Kind="Barcode" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                        <Label Content="BARCODE"/>
                        <Label x:Name="D_BarCode" Content="01234567891" Width="130"  VerticalContentAlignment="Center" HorizontalContentAlignment="Right" VerticalAlignment="Stretch" Foreground="Blue" FontSize="14" FontWeight="Bold"/>
                    </StackPanel>
                </Border>
                <Border Grid.Column="2" BorderThickness="1"  Margin="0,0,1,0">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <iconPacks:PackIconMaterial Margin="5,0,0,0"  Kind="Target" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                        <Label Content="USER"/>
                        <Label x:Name="D_User" Content="User" Width="100"  VerticalContentAlignment="Center" HorizontalContentAlignment="Right" VerticalAlignment="Stretch" Foreground="Blue" FontSize="14" FontWeight="Bold"/>
                    </StackPanel>
                </Border>
                <Border Grid.Column="3" BorderThickness="1"  Margin="0,0,1,0">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
                    </Border.BorderBrush>
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                        <iconPacks:PackIconMaterial Margin="5,0,0,0"  Kind="PackageUp" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                        <Label Content="MES "/>
                        <Label x:Name="D_MES" Content="message" Width="420"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" VerticalAlignment="Stretch" Foreground="Gray" FontSize="12" FontWeight="Bold"/>
                    </StackPanel>
                </Border>
            </Grid>
            <Grid Grid.Column="1" Grid.ColumnSpan="3" Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="120" />
                    <ColumnDefinition Width="120" />
                    <ColumnDefinition Width="120" />
                    <ColumnDefinition Width="120" />
                    <ColumnDefinition Width="120" />
                    <ColumnDefinition Width="120" />
                </Grid.ColumnDefinitions>
                <Border  Grid.Column="1" x:Name="FINALLAMP_15" BorderBrush="Black" BorderThickness="1" Background="#FFF9F9F9">
                    <Label Content="1.5bar Leak" HorizontalContentAlignment="Center" />
                </Border>
                <Border  Grid.Column="2" x:Name="FINALLAMP_50" BorderBrush="Black" BorderThickness="1" Background="#FFF9F9F9">
                    <Label Content="5.0bar Leak" HorizontalContentAlignment="Center" />
                </Border>
                <Border  Grid.Column="3" x:Name="FINALLAMP_PM" BorderBrush="Black" BorderThickness="1" Background="#FFF9F9F9">
                    <Label Content="Piston Moving" HorizontalContentAlignment="Center" />
                </Border>
                <Border  Grid.Column="0" x:Name="FINALLAMP_LT" BorderBrush="Black" BorderThickness="1" Background="#FFF9F9F9">
                    <Label Content="Lost Travel" HorizontalContentAlignment="Center" />
                </Border>
                <Border  Grid.Column="4" x:Name="FINALLAMP_PS" BorderBrush="Black" BorderThickness="1" Background="#FFF9F9F9">
                    <Label Content="Piston Stroke" HorizontalContentAlignment="Center" />
                </Border>
                <Border Visibility="Hidden" Grid.Column="5" x:Name="FINALLAMP_VA" BorderBrush="Black" BorderThickness="1" Background="#FFF9F9F9">
                    <Label Content="Vacuum" HorizontalContentAlignment="Center" Visibility="Hidden" />
                </Border>
            </Grid>
            <Rectangle Grid.Column="1" Grid.ColumnSpan="4" Grid.Row="0" Grid.RowSpan="2" Stroke="#FFE5E5E5"/>

        </Grid>
        <Grid Grid.Column="0" Grid.Row="1" Grid.RowSpan="3" x:Name="MenuGrid">
            <GroupBox Grid.Column="0" Grid.Row="0" Grid.RowSpan="4" Header="Menu" >
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="2*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="170"/>
                    </Grid.RowDefinitions>
                    <StackPanel Grid.Row="0" Orientation="Vertical">
                        <Button Margin="3,3,3,3" x:Name="Btn_USER"  >
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="Account" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="USER" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_OPEN">
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="OpenInApp" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="OPEN" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_EDIT">
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="Textbox" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="EDIT" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_TEST">
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="Run" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="TEST" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_RESULT"  >
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="ChartLine" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="RESULT" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_Log" Click="Btn_Log_Click"  >
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="History" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="LOG" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                        <Button Margin="3,3,3,3" x:Name="Btn_SELFTEST">
                            <StackPanel Orientation="Horizontal">
                                <iconPacks:PackIconMaterial Kind="Check" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                <Label Content="SELF TEST" Width="120" HorizontalContentAlignment="Center" FontSize="14" />
                            </StackPanel>
                        </Button>
                    </StackPanel>
                    <Grid Grid.Column="0" Grid.Row="1">
                        <Border BorderThickness="1" BorderBrush="#FFB5FF6B" >
                            <Grid Margin="1,0,-1,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1.5*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                </Grid.RowDefinitions>
                                <Label Grid.Column="0" Grid.Row="0" Content="Force(N)" />
                                <Label Grid.Column="0" Grid.Row="1" Content="Displacement(mm)" />
                                <Label Grid.Column="0" Grid.Row="2" Content="PRI Ari(bar)"/>
                                <Label Grid.Column="0" Grid.Row="3" Content="SEC Air(bar)"  />
                                <Label Grid.Column="0" Grid.Row="4" Content="PRI Vac(mmhg)" />
                                <Label Grid.Column="0" Grid.Row="5" Content="SEC Vac(mmhg)" />
                                <Label Grid.Column="0" Grid.Row="6" Content="PLC HOME" />
                                <TextBox Grid.Column="1" Grid.Row="0" x:Name="CH0" BorderBrush="{x:Null}" />
                                <TextBox Grid.Column="1" Grid.Row="1" x:Name="CH1"  BorderBrush="{x:Null}"/>
                                <TextBox Grid.Column="1" Grid.Row="2" x:Name="CH2"  BorderBrush="{x:Null}"/>
                                <TextBox Grid.Column="1" Grid.Row="3" x:Name="CH3"  BorderBrush="{x:Null}"/>
                                <TextBox Grid.Column="1" Grid.Row="4" x:Name="CH4"  BorderBrush="{x:Null}"/>
                                <TextBox Grid.Column="1" Grid.Row="5" x:Name="CH5"  BorderBrush="{x:Null}"/>
                                <TextBox Grid.Column="1" Grid.Row="6" x:Name="PLCHOME"  BorderBrush="{x:Null}"/>
                            </Grid>
                        </Border>
                    </Grid>
                    <Grid Grid.Column="0" Grid.Row="2">
                        <ni:BooleanButton x:Name="Buttton_Quit" Content="Quit" Height="40"  Click="Buttton_Quit_Click" Margin="10,105,10,15" />
                        <Image  Height="34"  Width="35" Source="pack://siteoforigin:,,,/Resources/Shutdown-42.png" Stretch="Fill" RenderTransformOrigin="0.55,2.265" Margin="14,108,166,18"/>
                        <ProgressBar x:Name="HDDUsage" HorizontalAlignment="Left" Height="6"  VerticalAlignment="Top" Width="209" Margin="6,94,0,0"/>
                        <ProgressBar x:Name="MEMUsage" HorizontalAlignment="Left" Height="5"  VerticalAlignment="Top" Width="209" Margin="6,63,0,0"/>
                        <Label Content="Memory" HorizontalAlignment="Left" Height="25"  VerticalAlignment="Top" Width="60" Margin="5,33,0,0"/>
                        <Label Content="HDD" HorizontalAlignment="Left" Height="25"  VerticalAlignment="Top" Width="60" Margin="5,69,0,0"/>
                        <ProgressBar x:Name="CPUUsage" HorizontalAlignment="Left" Height="6" VerticalAlignment="Top" Width="208" Margin="7,29,0,0"/>
                        <Label Content="CPU" HorizontalAlignment="Left" Height="25"  VerticalAlignment="Top" Width="60" Margin="5,4,0,0"/>
                    </Grid>
                </Grid>
            </GroupBox>
        </Grid>
        <Grid Grid.Column="0" Grid.ColumnSpan="5" Grid.Row="4"  x:Name="StatusGrid" HorizontalAlignment="Left" VerticalAlignment="Top" Background="#FF403075">
            <StatusBar HorizontalAlignment="Stretch" Height="26" VerticalAlignment="Top" Background="#FF403075" >
                <StatusBarItem x:Name="StripMode" Content="MANUAL" Height="26" VerticalAlignment="Top" Foreground="#FFC7C7C7" Background="Transparent"/>
                <StatusBarItem x:Name="StripReset" Content="RESET" Height="26" VerticalAlignment="Top" Width="54" Foreground="#FFF9E301"/>
                <StatusBarItem x:Name="StripError" Content="ERROR" Height="26" VerticalAlignment="Top" Width="68" Foreground="#FFFF4702"/>
                <ni:LED x:Name="StripCosmo1" Content="LED" FalseContent="Off" HorizontalAlignment="Left" Height="13" TrueContent="On" VerticalAlignment="Top" Width="13" RenderTransformOrigin="-12.1,0.8" TrueBrush="#FFF7F703" FalseBrush="#FFF7C9BE"/>
                <ni:LED x:Name="StripCosmo2" Content="LED" FalseContent="Off" HorizontalAlignment="Left" Height="13" TrueContent="On" VerticalAlignment="Top" Width="13" RenderTransformOrigin="-12.1,0.8" TrueBrush="#FFF7F703" FalseBrush="#FFF7C9BE"/>
                <ni:LED x:Name="StripPLCAlive" Content="LED" FalseContent="Off" HorizontalAlignment="Left" Height="13" TrueContent="On" VerticalAlignment="Top" Width="13" RenderTransformOrigin="-12.1,0.8" TrueBrush="#FFF7F703" FalseBrush="#FFF7C9BE"/>
                <ni:LED x:Name="StripDAQSave" Content="LED" FalseContent="Off" HorizontalAlignment="Left" Height="13" TrueContent="On" VerticalAlignment="Top" Width="13" RenderTransformOrigin="-12.1,0.8" TrueBrush="#FFF7F703" FalseBrush="#FFF7C9BE"/>

                <TextBox x:Name="TESTNAME" Width="682" VerticalContentAlignment="Center" HorizontalContentAlignment="Left" HorizontalAlignment="Left" Height="22" TextWrapping="Wrap" Text="System Operation Message..." VerticalAlignment="Top" Background="{x:Null}" Foreground="#FFF5E007"/>

                <ni:LED x:Name="Led_LDS" Content="LED" FalseContent="Off" HorizontalAlignment="Left" Height="15"  TrueContent="On" VerticalAlignment="Top" Width="15"/>
                <Label Content="LDS" HorizontalAlignment="Left" Height="26" VerticalAlignment="Center" Width="30" Background="{x:Null}" HorizontalContentAlignment="Left" VerticalContentAlignment="Stretch" Foreground="#FFDFF122"/>
                <ni:LED x:Name="Led_PLC" Content="LED" FalseContent="Off" HorizontalAlignment="Left" Height="15"  TrueContent="On" VerticalAlignment="Top" Width="15" RenderTransformOrigin="-12.1,0.8"/>
                <Label Content="PLC" HorizontalAlignment="Left" Height="26"  VerticalAlignment="Center" Width="30" Background="{x:Null}" HorizontalContentAlignment="Left" VerticalContentAlignment="Stretch" Foreground="#FFDFF122"/>
                <ni:LED x:Name="Led_Cosmo" Content="LED" FalseContent="Off" HorizontalAlignment="Left" Height="15" TrueContent="On" VerticalAlignment="Top" Width="15"/>
                <Label Content="COSMO" HorizontalAlignment="Left" Height="26"  VerticalAlignment="Center" Width="55" Background="{x:Null}" HorizontalContentAlignment="Left" VerticalContentAlignment="Stretch" Foreground="#FFDFF122"/>
                <ni:LED x:Name="Led_DAQHS" Content="LED" FalseContent="Off" HorizontalAlignment="Left" Height="15" TrueContent="On" VerticalAlignment="Top" Width="15" BorderBrush="#FF020202"/>
                <Label Content="DAQ HighSpeed" HorizontalAlignment="Left" Height="26"  VerticalAlignment="Center" Width="100" Background="{x:Null}" HorizontalContentAlignment="Left" VerticalContentAlignment="Stretch" Foreground="#FFDFF122"/>
            </StatusBar>
        </Grid>
        <Grid Grid.Column="1" Grid.Row="1" Grid.RowSpan="2" x:Name="WorkGrid" >
            <TabControl x:Name="MainSubScreen">
                <TabItem x:Name="LostTravel1" Header="LTP">
                    <Grid >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="140"/>
                        </Grid.RowDefinitions>
                        <ni:Graph Grid.Column="0" Grid.Row="0" x:Name="Graph_LostTravelP" RenderMode="Hardware"  >
                            <ni:Graph.Axes>
                                <ni:AxisDouble x:Name="LTV_AxisY" Orientation="Vertical" Label="PRI(bar)" Range="0.0, 1.5, System.Double" >
                                    <ni:AxisDouble.LabelTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding}" FontSize="16" Foreground="Red"/>
                                        </DataTemplate>
                                    </ni:AxisDouble.LabelTemplate>
                                    <ni:AxisDouble.MinorDivisions>
                                        <ni:RangeDivisions/>
                                    </ni:AxisDouble.MinorDivisions>
                                    <ni:AxisDouble.MajorDivisions>
                                        <ni:RangeLabeledDivisions LabelBrush="Red" Mode="Count: 5" >
                                            <ni:RangeLabeledDivisions.LabelPresenter>
                                                <ni:GeneralValueFormatter Format="N1" ShortFormat="{x:Null}"/>
                                            </ni:RangeLabeledDivisions.LabelPresenter>
                                        </ni:RangeLabeledDivisions>
                                    </ni:AxisDouble.MajorDivisions>
                                </ni:AxisDouble>
                                <ni:AxisDouble x:Name="LTH_AxisX" Orientation="Horizontal" Label="Displacement(mm)" Range="0.0, 6.0, System.Double">
                                    <ni:AxisDouble.LabelTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding}" FontSize="14" Foreground="Blue"/>
                                        </DataTemplate>
                                    </ni:AxisDouble.LabelTemplate>
                                    <ni:AxisDouble.MajorDivisions>
                                        <ni:RangeLabeledDivisions LabelBrush="Blue" Mode="Count: 7">
                                            <ni:RangeLabeledDivisions.LabelPresenter>
                                                <ni:GeneralValueFormatter Format="N1" ShortFormat="{x:Null}"/>
                                            </ni:RangeLabeledDivisions.LabelPresenter>
                                        </ni:RangeLabeledDivisions>
                                    </ni:AxisDouble.MajorDivisions>
                                </ni:AxisDouble>
                            </ni:Graph.Axes>
                            <ni:Graph.Plots>
                                <ni:Plot x:Name="LT_PlotPri" Label="PriAir" AdjustVerticalScale="False" AdjustHorizontalScale="True" VerticalScale="{Binding ElementName=LTV_AxisY}" HorizontalScale="{Binding ElementName=LTH_AxisX}"  >
                                    <ni:LinePlotRenderer Stroke="Red" StrokeThickness="1"/>
                                </ni:Plot>
                            </ni:Graph.Plots>
                            <ni:Graph.Children>
                                <ni:MultiPlotCursor x:Name="LT_PRI_1" Label="CutOffHole" CrosshairBrush="Blue" Foreground="Blue" TargetBrush="Red" Cursor="Hand" LabelVisibility="Visible" TargetShape="Plus" TargetSize="15,15"  >
                                    <ni:MultiPlotCursor.AxisValue>
                                        <System:Double>5</System:Double>
                                    </ni:MultiPlotCursor.AxisValue>
                                    <ni:MultiPlotCursor.ValuePresenter>
                                        <ni:ValueFormatterGroup>
                                            <ni:ValueFormatterGroup.DefaultFormatter>
                                                <ni:GeneralValueFormatter x:Name="valueFormatter" Format="N1" FontSize="20" FontStretch="UltraExpanded" FontStyle="Normal" FontWeight="Bold" ShortFormat="" Background="#FF6E1A1A" FrameworkElement.FlowDirection="RightToLeft" FontFamily="Century"/>
                                            </ni:ValueFormatterGroup.DefaultFormatter>
                                        </ni:ValueFormatterGroup>
                                    </ni:MultiPlotCursor.ValuePresenter>
                                </ni:MultiPlotCursor>
                            </ni:Graph.Children>
                        </ni:Graph>
                        <ni:Graph Grid.Column="0" Grid.Row="1" x:Name="Graph_LostTravelF" RenderMode="Hardware" >
                            <ni:Graph.Axes>
                                <ni:AxisDouble x:Name="LTF_AxisY" Orientation="Vertical" Label="FLO(bar)" Range="0.0, 3.3, System.Double">
                                    <ni:AxisDouble.LabelTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding}" FontSize="16" Foreground="Red"/>
                                        </DataTemplate>
                                    </ni:AxisDouble.LabelTemplate>
                                    <ni:AxisDouble.MinorDivisions>
                                        <ni:RangeDivisions/>
