                                        </ni:RangeLabeledDivisions>
                                    </ni:AxisDouble.MajorDivisions>
                                </ni:AxisDouble>
                                <ni:AxisDouble x:Name="X_Time2" Orientation="Horizontal" Label="Time(sec)" Range="0, 20.0, System.Double">
                                    <ni:AxisDouble.LabelTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding}" FontSize="16" Foreground="Blue"/>
                                        </DataTemplate>
                                    </ni:AxisDouble.LabelTemplate>
                                    <ni:AxisDouble.MinorDivisions>
                                        <ni:RangeDivisions/>
                                    </ni:AxisDouble.MinorDivisions>
                                    <ni:AxisDouble.MajorDivisions>
                                        <ni:RangeLabeledDivisions LabelBrush="Blue" Mode="Auto" >
                                            <ni:RangeLabeledDivisions.LabelPresenter>
                                                <ni:GeneralValueFormatter Format="N1" ShortFormat="{x:Null}"/>
                                            </ni:RangeLabeledDivisions.LabelPresenter>
                                        </ni:RangeLabeledDivisions>
                                    </ni:AxisDouble.MajorDivisions>
                                </ni:AxisDouble>
                            </ni:Graph.Axes>
                            <ni:Graph.Plots>
                                <ni:Plot x:Name="PriOutlet2" Label="Primary Outlet air"   VerticalScale="{Binding ElementName=V_Air}" HorizontalScale="{Binding ElementName=X_Time2}"   >
                                    <ni:LinePlotRenderer Stroke="Red" StrokeThickness="1"/>
                                </ni:Plot>
                                <ni:Plot x:Name="SecOutlet2" Label="Secondary(flo) Outlet Vacuum" VerticalScale="{Binding ElementName=V_Vacuum2}" HorizontalScale="{Binding ElementName=X_Time2}"  >
                                    <ni:LinePlotRenderer Stroke="Blue" StrokeThickness="1"/>
                                </ni:Plot>
                            </ni:Graph.Plots>
                            <ni:Graph.Children>
                                <ni:MultiPlotCursor x:Name="LTVH_FLO" Label="Check" CrosshairBrush="Blue" Foreground="Blue" TargetBrush="Red" Cursor="Hand" LabelVisibility="Visible" TargetShape="Plus" TargetSize="25,25"  >
                                    <ni:MultiPlotCursor.AxisValue>
                                        <System:Double>5</System:Double>
                                    </ni:MultiPlotCursor.AxisValue>
                                    <ni:MultiPlotCursor.ValuePresenter>
                                        <ni:ValueFormatterGroup>
                                            <ni:ValueFormatterGroup.DefaultFormatter>
                                                <ni:GeneralValueFormatter x:Name="valueFormatter12" Format="N2" FontSize="20" FontStretch="UltraExpanded" FontStyle="Normal" FontWeight="Bold" ShortFormat="" Background="#FF6E1A1A" FrameworkElement.FlowDirection="RightToLeft" FontFamily="Century"/>
                                            </ni:ValueFormatterGroup.DefaultFormatter>
                                        </ni:ValueFormatterGroup>
                                    </ni:MultiPlotCursor.ValuePresenter>
                                </ni:MultiPlotCursor>
                            </ni:Graph.Children>
                        </ni:Graph>
                        <!-- 결과 정보 표시-->
                        <Border Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="3"  Margin="2,2,2,2">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="40"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="40"/>
                                    <RowDefinition Height="40"/>

                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="120"/>
                                    <ColumnDefinition Width="145"/>
                                    <ColumnDefinition Width="150"/>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Border Grid.Column="0" Grid.ColumnSpan="9" Grid.Row="0" Margin="1,1,1,1" >
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                        <iconPacks:PackIconMaterial Margin="5,0,0,0"  Kind="Airplay" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                        <Label Content="iMEB VACUUM HOLD'G TEST - SEC " VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="24" FontStyle="Italic" FontWeight="ExtraBlack"/>
                                    </StackPanel>
                                </Border>

                                <Border Grid.Column="0" Grid.Row="1" Margin="1,1,1,1" BorderThickness="1" BorderBrush="Gray">
                                    <Label Content="진공압(mmHG)" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                                </Border>
                                <Border Grid.Column="0" Grid.Row="2" Grid.RowSpan="2" Margin="1,1,1,1">
                                    <Label  x:Name="VH_SECTestVacuum"  Content="500.0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="22" Foreground="Blue" FontWeight="Bold" />
                                </Border>

                                <Border Grid.Column="1" Grid.Row="1" Margin="1,1,1,1" BorderThickness="1" BorderBrush="Gray">
                                    <Label Content="공압(bar)" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                                </Border>
                                <Border Grid.Column="1" Grid.Row="2" Grid.RowSpan="2" Margin="1,1,1,1">
                                    <Label x:Name="VH_SECTestBar" Content="3.0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="22" Foreground="Blue" FontWeight="Bold" />
                                </Border>

                                <Border Grid.Column="2" Grid.Row="1" Margin="1,1,1,1" BorderThickness="1" BorderBrush="Gray">
                                    <Label x:Name="VH_TestCheckTimeSec" Content="10초후 측정기준" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                                </Border>

                                <Border Grid.Column="2" Grid.Row="2"  Margin="1,1,1,1">
                                    <StackPanel Orientation="Horizontal">
                                        <iconPacks:PackIconMaterial Margin="5,0,0,0"  Kind="ArrangeBringForward" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                        <Label Content="최소"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                                        <Label x:Name="R_VH_FLOMin" Content="38.0"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                                        <Label Content="mmHg" VerticalContentAlignment="Center" />
                                    </StackPanel>
                                </Border>
                                <Border Grid.Column="2" Grid.Row="3"  Margin="1,1,1,1">
                                    <StackPanel Orientation="Horizontal">
                                        <iconPacks:PackIconMaterial Margin="5,0,0,0" Kind="ArrangeBringForward" Rotation="180" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                        <Label Content="최대"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                                        <Label x:Name="R_VH_FLOMax"  Content="380.0"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                                        <Label Content="mmHg" VerticalContentAlignment="Center" />
                                    </StackPanel>
                                </Border>

                                <Border Grid.Column="3" Grid.Row="1" Margin="1,1,1,1" BorderThickness="1" BorderBrush="Gray">
                                    <Label Content="측정값(mmHg)" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                                </Border>
                                <Border Grid.Column="3" Grid.Row="2" Grid.RowSpan="2" Margin="1,1,1,1">
                                    <Label x:Name="VH_SECResult" Content="0.00" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="22" Foreground="Blue" FontWeight="Bold" />
                                </Border>

                                <Border Grid.Column="4"  Grid.Row="1" Margin="1,1,1,1" BorderThickness="1" BorderBrush="Gray">
                                    <Label Content="시험시간(초)" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                                </Border>
                                <Border Grid.Column="4"  Grid.Row="2" Grid.RowSpan="2" Margin="1,1,1,1">
                                    <Label x:Name="VH_SECTesTime" Content="0.0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="22" Foreground="Blue" FontWeight="Bold" />
                                </Border>

                                <Border Grid.Column="5" Grid.ColumnSpan="5" Grid.Row="1" Margin="1,1,1,1" BorderThickness="1" BorderBrush="Gray">
                                    <Label Content="시험판정" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                                </Border>
                                <Border Grid.Column="5" Grid.ColumnSpan="5" Grid.Row="2" Grid.RowSpan="2" Margin="1,1,1,1">
                                    <Label x:Name="VH_SECTotalResult" Content="OK" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="32" Foreground="Blue" FontWeight="Bold" />
                                </Border>

                            </Grid>
                        </Border>
                    </Grid>
                </TabItem>
                <TabItem x:Name="SysLog">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="30"/>
                        </Grid.RowDefinitions>
                        <TabControl Grid.Row="0" Margin="2" Name="MainTabControl" ItemsSource="{Binding FileMonitors}" SelectedItem="{Binding SelectedItem}">
                            <TabControl.ItemTemplate>
                                <DataTemplate DataType="TLog:FileMonitorViewModel">
                                    <TextBlock Text="{Binding FileName}" ToolTip="{Binding FilePath}" FontSize="12" />
                                </DataTemplate>
                            </TabControl.ItemTemplate>
                            <TabControl.ContentTemplate>
                                <DataTemplate DataType="TLog:FileMonitorViewModel">
                                    <Grid>
                                        <TextBox IsReadOnly="True" FontFamily="{Binding DataContext.Font, RelativeSource={RelativeSource AncestorType=Window}}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" UndoLimit="0" IsUndoEnabled="False" Text="{Binding Contents}" TextChanged="TextBoxBase_OnTextChanged" />
                                        <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="{Binding FileExists, Converter={StaticResource VisibilityBoolConverter}}" >
                                            <Border CornerRadius="3" Background="#BBBB0000">
                                                <Border.Effect>
                                                    <DropShadowEffect  RenderingBias="Quality" Opacity="0.6" BlurRadius="3" ShadowDepth="4" />
                                                </Border.Effect>
                                            </Border>
                                            <Label Foreground="White" FontSize="20" Padding="30,10,30,15">File Does Not Exist</Label>
                                        </Grid>
                                    </Grid>
                                </DataTemplate>
                            </TabControl.ContentTemplate>
                        </TabControl>
                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <Label Grid.Column="0" VerticalContentAlignment="Center" >Last Updated:</Label>
                            <TextBlock Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Text="{Binding LastUpdated}"/>
                            <Label Grid.Column="2" HorizontalAlignment="Right" VerticalContentAlignment="Center" Visibility="{Binding SelectedItem}">Encoding:</Label>
                            <ComboBox Grid.Column="3" Margin="3" SelectedValue="{Binding SelectedItem.EncodingName}" Visibility="{Binding SelectedItem, Converter={StaticResource NullVisibilityConverter}}"
                                      SelectedValuePath="Name" DisplayMemberPath="DisplayName" ItemsSource="{Binding Encodings}"/>
                        </Grid>
                    </Grid>
                </TabItem>
                <TabItem x:Name="BCConfig">
                    <Grid>

                        <Border BorderBrush="DarkBlue" BorderThickness="1" Margin="1,0,0,0">
                            <Grid>
                                <TabControl>
                                    <TabItem Header="System">
                                        <wpg:WpfPropertyGrid x:Name="PG_System" 
                                                 Margin="2,2,2,2" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                                                 HelpVisible="True" ToolbarVisible="True" PropertySort="Categorized"                                                 
                                                 />
                                    </TabItem>
                                    <TabItem Header="Test" >
                                        <wpg:WpfPropertyGrid x:Name="PG_Test" 
                                                 Margin="2,2,2,2" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                                                 HelpVisible="True" ToolbarVisible="True" PropertySort="Categorized"                                                 
                                                 />
                                    </TabItem>
                                </TabControl>
                            </Grid>
                        </Border>
                    </Grid>
                </TabItem>
            </TabControl>
        </Grid>
        <Expander Grid.Column="1" Grid.Row="3" Header="RESULT" ExpandDirection="Up" IsExpanded="False" Cursor="Arrow" >
            <Grid Background="#FFE5E5E5" >
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <DataGrid x:Name="TestResultGrid"
                          Padding="15" ColumnHeaderHeight="60" RowHeight="40"
                          HorizontalAlignment="Left" VerticalAlignment="Top" Height="760">
                </DataGrid>
            </Grid>
        </Expander>
    </Grid>
</Controls:MetroWindow>
